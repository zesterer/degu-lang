<machine id = "CommentCleaner" entry = "nocomment">
	<state id = "noncomment">
		<condition type = "alphanumeric" input = "null">
			<action type = "symbol-append" data = "input"></action>
			<action type = "symbol-new" data = "input"></action>
			<action type = "state-change" data = "nocomment"></action>
		</condition>
	</state>
</machine>
